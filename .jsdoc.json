{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["src", "README.md"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": true
    },
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "template": "node_modules/loke-jsdoc-theme",
        "recurse": true
    }
}